{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": 8
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "babel-plugin-transform-quasiquote",
    "@babel/plugin-syntax-jsx"
  ],
  "parserOpts": {
    "allowImportExportEverywhere": true
  }
}
